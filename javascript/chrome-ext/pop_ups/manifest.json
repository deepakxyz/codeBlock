{
    "manifest_version": 2,
    "name": "Pop-up Extension",
    "version":"2.0",
    "content_scripts":[
        {
            "matches":[
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action":{
        "default_icon":"in.png",
        "default_popup":"popup.html",
        "default_title":"A popup will come here"
    }
}